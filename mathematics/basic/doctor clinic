import java.util.*;
import java.lang.*;
import java.io.*;

class doctorclinic{
	public static void main (String[] args) {
		   Scanner s=new Scanner(System.in);
		   System.out.println("enter the number of test cases");
		   int t=s.nextInt();
		   for(int i=0;i<t;i++){
		          System.out.println("enter the number of paitents");
		          int n=s.nextInt();
			        System.out.println("enter the waiting time");
	            int x=s.nextInt();	    
		           System.out.println("last patient have to wait for: "+(((n-1)*10)-((n-1)*x))+"min");
		}
		
	}
}
